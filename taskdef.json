{
    "family": "${APP_NAME}",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "${TASK_ROLE_ARN}",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "<IMAGE_URI>",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ]
}

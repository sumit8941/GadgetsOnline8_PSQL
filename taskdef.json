{
    "family": "${APP_NAME}",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "${TASK_ROLE_ARN}",
    "containerDefinitions": [
        {
            "name": "${APP_NAME}",
            "image": "<IMAGE1_NAME>",
            "portMappings": [
                {
                    "containerPort": 81,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ]
}
